function FindProxyForURL(url, host) {
   // if matched the list , send to a specific proxy.
   var match = false;
   for(var i = 0; i < domains.length; i++) {
     if (host.endsWith(domains[i])) {
        match = true;
        break;
     }
   }
   if(match) {
    return proxy;
   }
   return direct;
}
